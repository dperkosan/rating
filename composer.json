    "license": "MIT",
    "authors": [
        {
            "name": "Damir Perkosan",
            "email": "dperkosan@gmail.com",
            "homepage": "https://github.com/dperkosan"
        }
    ],
    "homepage": "https://github.com/dperkosan/rating",
    "keywords": ["Laravel", "Rating"],
    "require": {
